<!-- begin:: Content -->
						<div class="kt-container  kt-container--fluid  kt-grid__item kt-grid__item--fluid">

							<!--begin: Row-->
							<div class="row">
							    <% if (team.users.length == 0) { %>
    							    <div class="kt-portlet kt-portlet--mobile">
    							        <div class="kt-portlet__body">
                                            <div class="kt-heading kt-heading--center">Oops... No employee in this team yet.</div>
                                        </div>
                                    </div>
                                <% } else { %>
							    <% team.users.forEach(function(teamUsers) { %>
								<div class="col-xl-4 col-lg-6">
									<div class="kt-portlet kt-portlet--height-fluid">
										<div class="kt-portlet__body">
											<div class="kt-widget kt-widget--general-1">
												<div class="kt-media kt-media--lg kt-media--circle">
													<img src="/assets/media/users/100_4.jpg" alt="image">
												</div>
												<div class="kt-widget__wrapper">
													<div class="kt-widget__label">
														<a href="#" class="kt-widget__title">
															<%= teamUsers.username %>
														</a>
														<span class="kt-widget__desc">
															<%= teamUsers.profile %>
														</span>
													</div>
													<div class="kt-widget__toolbar">
														<a href="#" class="btn btn-default btn-sm btn-bold btn-upper">chat</a>
														<a href="custom/profile/overview-1.html" target="_blank" class="btn btn-default btn-sm btn-bold btn-upper">profile</a>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
								<% }); %>
								<% } %>
							</div>

							<!--end: Row-->
							
							<!--begin table-->
							
							<div class="kt-portlet kt-portlet--mobile">
								<div class="kt-portlet__head">
									<div class="kt-portlet__head-label">
										<h3 class="kt-portlet__head-title">
											<%= teamName.team_name %>'s Tasks
										</h3>
									</div>
								</div>
								<div class="kt-portlet__body">

									<!--begin: Datatable -->
									<table class="table table-striped- table-bordered table-hover table-checkable" id="">
										<thead>
											<tr>
												<th>ID</th>
						                        <th>Task Title</th>
						                        <th>Duration</th>
						                        <th>Status</th>
						                        <th>Created By</th>
						                        <th>Time Created</th>
						                        <th>Last Modified</th>
						                        <th>Assigned To</th>
						                        <th>Action</th>
											</tr>
										</thead>
										<tbody>
											<% tasks.forEach(function(task) { %>
											<tr>
												<td><%= task.id %></td>
						                        <td><%= task.title %></td>
						                        <td><%= task.duration %> Hour(s)</td>
						                        <td><span class="btn btn-label-primary btn-sm"><%= task.status %></span></td>
						                        <td><%= task.user.username %></td>
						                        <td><%= moment(task.createdAt).fromNow() %></td>
						                        <td><%= moment(task.updatedAt).fromNow() %></td>
						                        <% if (task.Employee == null) { %>
						                        <td><%= task.Team.team_name %></td>
						                        <% } else { %>
						                        <td><%= task.Employee %></td>
						                        <% } %>
						                        <td>
						                        	<div class="dropdown dropdown-inline">
					        							<button type="button" class="btn btn-hover-brand btn-elevate-hover btn-icon btn-sm btn-icon-md btn-circle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
					        								<i class="flaticon-more-1"></i>
					        							</button>
					    							<div class="dropdown-menu dropdown-menu-right">
					    								<% if (task.Employee == null) { %>
					    								<a class="dropdown-item" href="/todo/task/<%= task.id %>/<%= team.id %>/pick"><i class="la la-plus"></i> Pick up</a>
					    								<% } else { %>
					    								<a class="dropdown-item " href="/todo/task/<%= task.id %>/<%= task.employee %>/todo"><i class="la la-plus"></i> Todo</a>
					    								<a class="dropdown-item" href="/todo/task/<%= task.id %>/<%= task.employee %>/inprogress"><i class="la la-user"></i> InProgress</a>
					    								<a class="dropdown-item" href="/todo/task/<%= task.id %>/<%= task.employee %>/review"><i class="la la-cloud-download"></i> Review</a>
					    								<div class="dropdown-divider"></div>
					    								<a class="dropdown-item" href="/todo/task/<%= task.id %>/<%= task.employee %>/done"><i class="la la-cog"></i> Done</a>
					    								<% } %>
					    							</div>
											    </div>
						                        </td>
											</tr>
											<% }); %>
										</tbody>
									</table>
									<!--end: Datatable -->
								</div>
							</div>
							
							<!--end table-->
						</div>

						<!-- end:: Content -->